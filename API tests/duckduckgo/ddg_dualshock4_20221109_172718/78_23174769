<script type="text/javascript">
$(function() {
  $('.photo-slider').on('init', function(){
    $('photo-slider-loading').hide();
    $('.photo-slider, .thumb-slider').css('visibility', 'visible');
    $('.photo-slider .slick-cloned a').removeAttr('data-lightbox');
  }).on('beforeChange', function(event, slick, currentSlide, nextSlide){
    var $currentSlide = $(this).find('.slick-slide[data-slick-index="' + currentSlide + '"]');
    if ( $currentSlide.data('player') ) {
      $currentSlide.data('player').pauseVideo();
    }
    var $nextSlide = $(this).find('.slick-slide[data-slick-index="' + nextSlide + '"]');
    if ( $nextSlide.data('player') ) {
      $nextSlide.data('player').playVideo();
    }
    var $image = $nextSlide.find('img').attr('src');
    // if click in image carousel
    if ( $image == null ) {
        $image = $nextSlide.find('img').data('lazy')
    }
    addNewImage($image, 0);
  }).on('afterChange', function(event, slick, currentSlide){
    var $currentSlide = $(this).find('.slick-slide[data-slick-index="' + currentSlide + '"]');
    if ( $currentSlide.find('a').data('map') ) {
      $currentSlide.find('img').attr('src', $currentSlide.find('a').data('map'));
    }
  }).slick({
    dots: false,
    arrows: true,
    infinite: true,
    speed: 300,
    slidesToShow: 1,
    slidesToScroll: 1,
    centerMode: true,
    variableWidth: true,
    asNavFor: '.thumb-slider',
    nextArrow: '<a class="slick-next icon icon-arrow-right"></a>',
    prevArrow: '<a class="slick-prev icon icon-arrow-left"></a>',
    responsive: [{
      breakpoint: 1200,
      settings: {
        slidesToShow: 1,
        slidesToScroll: 1,
        infinite: true,
        dots: false
      }
    }]
  });

  $('.thumb-slider').on('init', function(){
    var $track = $(this).find('.slick-track');
    $track.css('transition', 'all 400ms ease-out');
  }).on('beforeChange', function(event, slick, currentSlide, nextSlide){
    var w = $(this).find('.slick-current').width();
    var $track = $(this).find('.slick-track');
    var lpadding = parseInt($track.parent().css('paddingLeft'));
    var curpos = $track.position().left;
    var newpos = curpos - lpadding + (w * (currentSlide - nextSlide));
    $(this).one('afterChange', function(event, slick, currentSlide){
      $track.css('transform', 'matrix(1, 0, 0, 1, ' + newpos + ', 0)');
    });
  });
  $('.thumb-slider').slick({
    slidesToShow: 9,
    slidesToScroll: 1,
    asNavFor: '.photo-slider',
    centerMode: true,
    focusOnSelect: true,
    variableWidth: true,
    dots: false,
    arrows: true,
    nextArrow: '<a class="slick-next icon icon-arrow-right"></a>',
    prevArrow: '<a class="slick-prev icon icon-arrow-left"></a>'
  });

  // lightbox images
  $('.lb-image').on('load', function () {
    var $image = $(this).attr('src');
      addNewImage($image, 1);
  });

  function addNewImage($image, $lightbox) {
    var $images_count = 4;
    var $image_seen_selector = $('#images-seen');
    var $countseen  = $image_seen_selector.data('countseen');
    // if user not seen all ad images
    if ($countseen != $images_count) {
      var $imagesSeenArray = $('#images-seen').data('imagesseen').split(',');
      // if is an image
      if ($image.indexOf("encuentra24") >= 0) {
        $imagesSeenArray = $imagesSeenArray.concat($image.split('/').pop());
        // if image is not be seen yet
        var $imagesSeenArrayUnique = $imagesSeenArray.filter(function(elem, index, self) {
          return index === self.indexOf(elem);
        });
        if ($imagesSeenArray.length == $imagesSeenArrayUnique.length) {
          $image_seen_selector.attr('data-imagesseen', $imagesSeenArrayUnique.join(','));
          $image_seen_selector.data('imagesseen', $imagesSeenArrayUnique.join(','));
          $image_seen_selector.attr('data-countseen', $imagesSeenArrayUnique.length);
          $image_seen_selector.data('countseen', $imagesSeenArrayUnique.length);
          var url = '';
          if ( $lightbox ) {
            url = "https://analytics.biv2.encuentra24.com/images_seen?t=yofZd84iB0d40%2FBaupK3xlMITnDgQ1%2B6kQRmRTJtGpt%2FBHc6vJ0phSHk2A3eGJhaXUaRcrteDjCvthhSgl8CDbyJ0Pj%2BJ5DSOlGPSca2Ii8xd6j3P24J6odLOoTWKg8k4dwhAsDVIeM3FYBzkbP3IC0x8jjE%2FaY5w8h5m8Gf4Otyfyhw9N8gBrvlWtFk6Ey1FvB0%2FXlSBjCDm8rNSCyv7EV9lMCafjPjSD63Is7M%2B6UXnYQdOZ9prjrUy%2Bl8E%2FzdkAcMIFVFoYFgAOfhZPlSxFBB17T3S8YIUcldFl3Deh3OO1Pg0TeZTBjPKp1YQhdh";
          }
          else {
            url = "https://analytics.biv2.encuentra24.com/images_seen?t=yofZd84iB0d40%2FBaupK3xlMITnDgQ1%2B6kQRmRTJtGpt%2FBHc6vJ0phSHk2A3eGJhaXUaRcrteDjCvthhSgl8CDbyJ0Pj%2BJ5DSOlGPSca2Ii8xd6j3P24J6odLOoTWKg8k4dwhAsDVIeM3FYBzkbP3IC0x8jjE%2FaY5w8h5m8Gf4Otyfyhw9N8gBrvlWtFk6Ey1FvB0%2FXlSBjCDm8rNSCyv7EV9lMCafjPjSD63Is7M%2B6UXnYQdOZ9prjrUy%2Bl8E%2FzdkAcMIFVFoYFgAOfhZPlSxMse3sKJ%2FOd%2BL29bk5h1I%2FSHzzrhgjoVTMoFTN7ZqJvT";
          }

          if ( url ) {
            $.ajax({
              'url': url + '&images=' + $imagesSeenArrayUnique.length + '&image_url=' + $image
            });
          }
        }
      }
    }
  }
});
</script>
