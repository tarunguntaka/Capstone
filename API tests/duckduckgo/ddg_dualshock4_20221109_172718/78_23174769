$(function(){
  // get all divs with a google tracking code
  $('div.e24googlead').each(function() {

    var id = $(this).attr('id');
    var slotname = $(this).attr('data-gg-slot');

    var width = parseInt($(this).css('width').replace('px', '').replace(' ', ''));
    var height = parseInt($(this).css('height').replace('px', '').replace(' ', ''));

    googletag.cmd.push(function() {
      googletag.defineSlot(slotname, [width, height], id).addService(googletag.pubads());
      googletag.pubads().enableSingleRequest();
      googletag.pubads().collapseEmptyDivs();
      googletag.enableServices();
    });
    googletag.cmd.push(function() { googletag.display(id); });
  });
});
