<script type="text/javascript">
$(function(){
  $(document).on('click', '.see-phone, .button_call, .open-whatsapp, .icon-favorite:not(.fav-highlighted), .tool-favorite:not(.fav-highlighted)', function(){
    var $this = $(this);
    // show suggestions modal
