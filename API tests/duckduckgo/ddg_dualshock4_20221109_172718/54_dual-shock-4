					var displayWidth = getDisplayWidth();

					var displayWidthToPlacementIds = function( contextPlacementMap ) {
						var placementIds = [];

						if ( contextPlacementMap.mobile && displayWidth < 1000 ) {
							placementIds.push( contextPlacementMap.mobile );
						}
						if ( contextPlacementMap.desktop && displayWidth >= 1000 ) {
							placementIds.push( contextPlacementMap.desktop );
						}
						if ( contextPlacementMap.all ) {
							placementIds.push( contextPlacementMap.all );
						}

						return placementIds;
					}

					var renderPlacement = function( placement ) {
						if ( 'object' === typeof placement.placement_ids && placement.div_id ) {
							displayWidthToPlacementIds( placement.placement_ids ).map( function( data ) {
								var placeholder = document.getElementById( placement.div_id );
								var element = document.createElement( 'div' );
