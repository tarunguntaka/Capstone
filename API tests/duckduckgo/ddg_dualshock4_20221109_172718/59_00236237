        <script>
             var userConsentRequired = false
             var allowCookieOperationsForFav = false;
             var allowCookieOperationsForRVP = false;
             (function(){
                 var locale = "en_US";
                 var allowedCategories = ["all", "sony-platform"];
                 var intersects = function (arr1, arr2) {
                     for (var i = 0; i < arr1.length; i++) {
                         if (arr2.indexOf(arr1[i]) >= 0) return true;
                     }
                     return false;
                 };
                 function triggerCustomEvent(event_name) {
                     var event = document.createEvent('Event');
                     event.initEvent(event_name, true, true);
                     document.dispatchEvent(event);
                 }
                 if(userConsentRequired) {
                     document.addEventListener("consent_given", function (e) {
                         var consent_cat = e.detail.categories.split(",");
                         var rvp_key = 'gwt_recently_viewed_products_' + locale;
                         if (intersects(consent_cat, allowedCategories)) {
                             allowCookieOperationsForFav = true;
                             allowCookieOperationsForRVP = true;
                         }
                         if (consent_cat.indexOf("gwt_favorite_products") > -1) {
                            allowCookieOperationsForFav = true;
                         }
                         if (consent_cat.indexOf(rvp_key) > -1) {
                            allowCookieOperationsForRVP = true;
                         }
                         if(allowCookieOperationsForFav || allowCookieOperationsForRVP){
                            triggerCustomEvent("consent_received");
                         } else {
                            triggerCustomEvent("consent_rejected");
                         }
                     });
                     document.addEventListener("consent_declined", function () {
                         allowCookieOperationsForFav = false;
                         allowCookieOperationsForRVP = false;
                         triggerCustomEvent("consent_rejected");
                     });
                 }
             })();
        </script>
        



    <script >
            var deviceType = (window.innerWidth > 480 && window.innerWidth < 769) ? {isTablet: true} : (window.innerWidth > 768 ? {isDesktop: true} : {isPhone: true});
            var loadSSTFontForMobile    =  true,
                loadSSTFontForTablet    =  true,
                loadSSTFontForDesktop   =  true,
                loadSSTFontMaster       =  true;

            window.shouldLoadSSTFonts   = loadSSTFontMaster &&
                                          ((deviceType.isTablet  && loadSSTFontForTablet)
                                        || (deviceType.isPhone   && loadSSTFontForMobile)
                                        || (deviceType.isDesktop && loadSSTFontForDesktop));

            var fontType = (function(){
                var supportsWoff2 = (function( win ){
                    if( !( "FontFace" in win ) )  return false;
                    var f = new win.FontFace( 't', 'url(data:application/font-woff2;charset=utf-8;base64,) format("woff2")', {});
                    f.load().catch(function() {});
                    return f.status == 'loading';
                })( this );

                function requiresTTF(){
                    return navigator.userAgent.toLowerCase().match(/android|vita|playstation/g) !== null;
                }

                if (supportsWoff2) {
                    return ".woff2";
                }
                else if(requiresTTF()){
                    return ".ttf";
                }
                else {
                    return ".woff";
                }
            })();

            function loadFont(fontFamily, url) {
                var font = new FontFace(fontFamily,  "url('" + url + fontType + "')");
                font.load();
                document.fonts.add(font);
            }

            if("FontFace" in window) {
                if(window.shouldLoadSSTFonts) {
                    loadFont("SST W01 Roman",  "//www.sony.com/static-assets/fonts/shf/v31/latinextended/fonts/SSTW02Roman");
                    loadFont("SST W01 Medium",  "//www.sony.com/static-assets/fonts/shf/v31/latinextended/fonts/SSTW02Medium");
                }
                
                loadFont('Sony-Support-Icons', '//www.sony.com/static-assets/fonts/shf/icons/support/v4/Sony-Support-Icons');
                
                loadFont('Sony-System-Icons', '//www.sony.com/static-assets/fonts/shf/icons/system/v23/Sony-System-Icons');
                
            } else {
                var str = [];
                if(window.shouldLoadSSTFonts) {
                    str.push("%3Clink rel='stylesheet' href='//www.sony.com/static-assets/fonts/shf/v31/latinextended/base.css'%3E");
                }
                
                    str.push("%3Clink rel='stylesheet' href='" + '//www.sony.com/static-assets/fonts/shf/icons/support/v4/Sony-Support-Icons.css' + "'%3E");
                
                    str.push("%3Clink rel='stylesheet' href='" + '//www.sony.com/static-assets/fonts/shf/icons/system/v23/Sony-System-Icons.css' + "'%3E");
                
                document.write(decodeURIComponent(str.join('')));
            }


    </script>

    
