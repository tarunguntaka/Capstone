<script type="text/javascript">
let mokaFindParentAnchor = function(elem) {
    if (elem.href) return elem;
    if (elem.parentElement) return mokaFindParentAnchor(elem.parentElement);
    return null;
}
var trackonomicsXidSet = function() {
	// Use function context as a static
	this.lastLinkId = this.lastLinkId || 0;
	var links = document.querySelectorAll('a');
	
	for (var i = 0; i < links.length; i++) {
		var link = links[i].getAttribute('href');
		if (!link) {
			// Mostly autotoc anchors etc.
			continue;
		}
		link = link.replace('%7Bxid%7D', '{xid}');
		
		if (link && link.indexOf && (link.indexOf('{xid}') != -1 || link.indexOf('amazon.com') != -1)) {
			var letter3 = this.lastLinkId % 26;
			var letter2 = Math.floor(this.lastLinkId / 26) % 26;
			var letter1 = Math.floor(this.lastLinkId / 676);
			var stringLinkId = String.fromCharCode('a'.charCodeAt(0) + letter1) + String.fromCharCode('a'.charCodeAt(0) + letter2) + String.fromCharCode('a'.charCodeAt(0) + letter3);
			
			links[i]._trxXid = 'fr' + Math.round(Date.now()/1000) + stringLinkId;
			var updatedLink = link.replace('{xid}', links[i]._trxXid);
			var trxnet = updatedLink.match(/\&___trxnet=([a-z]*)/);
			if (trxnet && trxnet.length == 2) {
				links[i].setAttribute('data-trxnetwork', trxnet[1]);
				updatedLink = updatedLink.replace(trxnet[0], '');
			} else if (link.indexOf('amazon.com') != -1) {
				links[i].setAttribute('data-trxnetwork', 'az');
			}
			links[i].setAttribute('href', updatedLink);
			this.lastLinkId++;
			
			links[i]._trxReport = function(e) {
				var trueAnchorElem = mokaFindParentAnchor(e.target);
				var trxNetwork = trueAnchorElem.getAttribute('data-trxnetwork');
				if (!trxNetwork || trxNetwork == '') {
					trxNetwork = 'other';
				}
				if (e.type != 'contextmenu' && trxNetwork != 'az') {
					// Existing non-TRX handler will open AZ
					e.preventDefault();
					window.open(trueAnchorElem.href);
				}
				var pageVars = {};
				window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m, key, value) {
					pageVars[key] = value;
				});
				var attrForwarding = {};
				if (pageVars.utm_source) attrForwarding.utm_source = pageVars.utm_source;
				if (pageVars.utm_campaign) attrForwarding.utm_campaign = pageVars.utm_campaign;
				if (pageVars.utm_medium) attrForwarding.utm_medium = pageVars.utm_medium;
				if (pageVars.gclid) attrForwarding.gclid = pageVars.gclid;
				if (pageVars.msclkid) attrForwarding.msclkid = pageVars.msclkid;
				var metadata = {
					"author":"Chris Hoffman",					"referrer": document.referrer
				};
				if (window._htg_tags) {
					var currentTagIdx = 1;
					window._htg_tags.forEach((t) => { 
						var forbiddenTags = [
							'bigpic',
							'googlenewsfeatures',
							'splashbox',
							'rightsplashbox',
							'apply_overlay_badges',
							'hide_inline_content_ads'
						]
						if (forbiddenTags.indexOf(t) == -1) {
							metadata["tag" + currentTagIdx] = t;
							currentTagIdx++;
						}
					});
				}
				
				var trxData = {
					"env": "lifeSavvymedia_c25c1",
					"id": trueAnchorElem._trxXid,
					"app_key": "gsd65fgr0hgTTT",
					"property_id": "PROPERTY_ID",
					"customer_id": "lifeSavvymedia_c25c1_howtogeek",
					"cidp1": "lifeSavvymedia_c25c1",
					"cidp2": "howtogeek",
					"event_type": "click",
					"network": trxNetwork,
					"data": encodeURIComponent(JSON.stringify({
						   "attr_forwarding": attrForwarding,
						   "metadata": metadata,
						   "property_id":"PROPERTY_ID",
						   "customer_id":"lifeSavvymedia_c25c1_howtogeek",
						   "cidp1":"lifeSavvymedia_c25c1",
						   "cidp2":"howtogeek",
						   "source": document.location.href.split('?')[0],
						   "event_type": "click",
						   "referrer": document.referrer,
						   "uuid": trueAnchorElem._trxXid,
						   "link_replaced":false,
						   "anchor_orig": trueAnchorElem.href.replace(new RegExp('&[a-zA-Z0-9]+=xid:' + trueAnchorElem._trxXid), ''),
						   "anchor_href": trueAnchorElem.href,
						   "page_title": document.title,
						   "spot_id":"",
						   "network": trxNetwork
					}))
				};
				let trxxmlhttp = new XMLHttpRequest();
				trxxmlhttp.open("POST", 'https://write.trackonomics.net/prod/write', true);
				trxxmlhttp.send(JSON.stringify(trxData));
				
				// Ours too:
				var generalLink = trueAnchorElem.href.replace(/https?:\/\/(www.)?/, '');
				var htg = -1;
				var htgm = document.cookie.match('htg=([0-9]*)');
				if (htgm && htgm.length == 2) {
					htg = htgm[1];
				}
				var widgetSource = trueAnchorElem.getAttribute('data-widgetsource');
				if (widgetSource) {
					if (generalLink.indexOf('?') != -1) {
						generalLink += '&widgetsource=' + encodeURIComponent(widgetSource);
					} else {
						generalLink += '?widgetsource=' + encodeURIComponent(widgetSource);
					}
				}
				var mokaAB = trueAnchorElem.getAttribute('data-moka_ab');
				if (mokaAB) {
					if (generalLink.indexOf('?') != -1) {
						generalLink += '&moka_ab=' + encodeURIComponent(mokaAB);
					} else {
						generalLink += '?moka_ab=' + encodeURIComponent(mokaAB);
					}
				}
				var trackDest = 'https://px1.lifesavvy.com/gt/howtogeek/240152/' + htg + '/' + generalLink;
				let xmlhttp = new XMLHttpRequest();
				xmlhttp.open("POST", trackDest, true);
				xmlhttp.send(JSON.stringify({
					extraArgs: {
						forwarded_referrer: document.referrer,
						trx_xid: trueAnchorElem._trxXid
					}
				}));
			}
			links[i].addEventListener('click', links[i]._trxReport);
			links[i].addEventListener('contextmenu', links[i]._trxReport);
		}
	}
}
document.addEventListener('DOMContentLoaded', trackonomicsXidSet);
window.addEventListener("click", function(e) {
	var trueAnchorElem = mokaFindParentAnchor(e.target);
	if (trueAnchorElem && trueAnchorElem.href && trueAnchorElem.href.indexOf('amazon.com') != -1 && trueAnchorElem.getAttribute('data-skiptrack') === null ) {
		e.preventDefault();

		window.open(trueAnchorElem.href);

		let xmlhttp = new XMLHttpRequest();
		var amzLink = trueAnchorElem.href.replace(/https?:\/\/(www.)?amazon.com\//, '');
		var htg = -1;
		var htgm = document.cookie.match('htg=([0-9]*)');
		if (htgm && htgm.length == 2) {
			htg = htgm[1];
		}
		var widgetSource = trueAnchorElem.getAttribute('data-widgetsource');
		if (widgetSource) {
			if (amzLink.indexOf('?') != -1) {
				amzLink += '&widgetsource=' + encodeURIComponent(widgetSource);
			} else {
				amzLink += '?widgetsource=' + encodeURIComponent(widgetSource);
			}
		}
		var mokaAB = trueAnchorElem.getAttribute('data-moka_ab');
		if (mokaAB) {
			if (amzLink.indexOf('?') != -1) {
				amzLink += '&moka_ab=' + encodeURIComponent(mokaAB);
			} else {
				amzLink += '?moka_ab=' + encodeURIComponent(mokaAB);
			}
		}
		var trackDest = 'https://px1.lifesavvy.com/a/howtogeek/240152/' + htg + '/' + amzLink;
		xmlhttp.open("POST", trackDest, true);
		xmlhttp.send(JSON.stringify({
			extraArgs: {
				forwarded_referrer: document.referrer
			}
		}));
		
			}
});
window.addEventListener("contextmenu", function(e) {
	var trueAnchorElem = mokaFindParentAnchor(e.target);
	if (trueAnchorElem && trueAnchorElem.href && trueAnchorElem.href.indexOf('amazon.com') != -1 && trueAnchorElem.getAttribute('data-skiptrack') === null) {
		// No need for preventdefault on this version - and no need to listen for finish
		var amzLink = trueAnchorElem.href.replace(/https?:\/\/(www.)?amazon.com\//, '');
		var htg = -1;
		var htgm = document.cookie.match('htg=([0-9]*)');
		if (htgm && htgm.length == 2) {
			htg = htgm[1];
		}
		var widgetSource = trueAnchorElem.getAttribute('data-widgetsource');
		if (widgetSource) {
			if (amzLink.indexOf('?') != -1) {
				amzLink += '&widgetsource=' + encodeURIComponent(widgetSource);
			} else {
				amzLink += '?widgetsource=' + encodeURIComponent(widgetSource);
			}
		}
		var mokaAB = trueAnchorElem.getAttribute('data-moka_ab');
		if (mokaAB) {
			if (amzLink.indexOf('?') != -1) {
				amzLink += '&moka_ab=' + encodeURIComponent(mokaAB);
			} else {
				amzLink += '?moka_ab=' + encodeURIComponent(mokaAB);
			}
		}
		var trackDest = 'https://px1.lifesavvy.com/a/howtogeek/240152/' + htg + '/' + amzLink;
		let xmlhttp = new XMLHttpRequest();
		xmlhttp.open("POST", trackDest, true);
		xmlhttp.send(JSON.stringify({
			extraArgs: {
				forwarded_referrer: document.referrer
			}
		}));
		
			}
});

let handleHtgHash = function() {
	if (document.location.hash.indexOf('#htg=') != -1) {
		let htgid = document.location.hash.match(/htg=([0-9]*)/);
		let uid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, (c) => {
			var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
			return v.toString(16);
		});
		if (htgid.length == 2) {
			document.cookie = 'htg=' + htgid[1] + '; expires=Thu, 9 Nov 2023 00:00:00 UTC; path=/';
			document.cookie = 'htgreader=' + uid + '; expires=Thu, 9 Nov 2023 00:00:00 UTC; path=/';
			document.cookie = 'htg_newsletter_subform_hide=1; expires=Thu, 9 Nov 2023 00:00:00 UTC; path=/';
		}

		history.pushState('', document.title, window.location.pathname);
	}
}();
</script>
<script>
	// Load all lazy-loaded images before someone prints:
	let loadAllLazy = () => {
		var imgs = document.querySelectorAll('[data-pagespeed-lazy-src]');
		imgs.forEach(e => e.src = e.attributes['data-pagespeed-lazy-src'].value);
	}
	// IE and FireFox:
	window.onbeforeprint = loadAllLazy;
	// Chrome and Safari:
	let mediaQueryList = window.matchMedia('print');
	mediaQueryList.addListener((mql) => {
    	if (mql.matches) loadAllLazy();
	});
</script>
<script type="text/javascript">
	(function() {
		if (document.referrer) {
			var x = document.getElementsByName('moka-pagerefer');
			for (var i = 0; i < x.length; i++) {
				x[i].value = document.referrer;
			}
		}
		
		document.querySelectorAll('.menu-item, #topmenu li a, .explore-menu-trigger').forEach(function(item) {
			item.addEventListener('click', function(e) {
				var menuLabel = this.innerText.replace('', '').trim();
				gtag('event', 'Menu Item', {
				  event_catgory: menuLabel,
				  event_label: menuLabel
				});
			}, false);
		});
	})();
	
			window._htg_bordeaux_remove_excess = function() {
			document.getElementById('floating-sidebar-area').style.display = 'block';
				
			[...document.getElementsByClassName('future_hybrid_sidebar_top_margin')].forEach(function(slot) {
				if (slot.offsetTop + slot.clientHeight > document.getElementsByClassName('entry-footer')[0].offsetTop) {
					slot.parentNode.removeChild(slot);
				}
			});
			[...document.getElementsByClassName('bordeaux-slot')].forEach(function(slot) {
				if (slot.offsetTop + slot.clientHeight > document.getElementsByClassName('entry-footer')[0].offsetTop) {
					slot.parentNode.removeChild(slot);
				}
			});
		};
		setTimeout(function() {
			window.addEventListener('scroll', window._htg_bordeaux_remove_excess);
		}, 800);
		
		document.addEventListener('DOMContentLoaded', () => {
			setTimeout(() => {
				if (bordeaux && bordeaux.log && bordeaux.log.history && bordeaux.log.history.forEach) {
					bordeaux.log.history.forEach((x) => {
						if (x && x.args && x.args.join && x.args.join().indexOf('Unable to parse the config response') != -1) {
							[...document.getElementsByClassName('bordeaux-slot')].forEach((slot) => slot.parentNode.removeChild(slot));
						}
					})
				}
			}, 1000);
		});
	</script>
